{
    "name": "NgChromeExtesnion",
    "version": "1.0",
    "description": "Chrome Extension made in angular",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "bookmarks"
    ],
    "background": {
        "scripts": [
            "background.js",
            "runtime.js"
        ]
    }
}